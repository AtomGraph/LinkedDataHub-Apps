    <triple>
      <uri>file:///user-guide/build-apps/</uri>
      <uri>http://purl.org/dc/terms/description</uri>
      <plainLiteral>Using LinkedDataHub as a low-code platform for Knowledge Graph applications</plainLiteral>
    </triple>
    <triple>
      <uri>file:///user-guide/build-apps/</uri>
