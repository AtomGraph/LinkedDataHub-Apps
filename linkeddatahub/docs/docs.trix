      <plainLiteral>Search for resources using text keywords</plainLiteral>
    </triple>
    <triple>
      <uri>file:///user-guide/search-data/</uri>
      <uri>http://www.w3.org/1999/02/22-rdf-syntax-ns#_1</uri>
      <uri>file:///user-guide/search-data/#content</uri>
    </triple>
    <triple>
      <uri>file:///user-guide/search-data/#content</uri>
