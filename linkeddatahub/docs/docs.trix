    <triple>
      <uri>file:///get-started/setup/</uri>
      <uri>http://purl.org/dc/terms/description</uri>
      <plainLiteral>Setup LinkedDataHub on your local machine or access it in the cloud</plainLiteral>
    </triple>
    <triple>
      <uri>file:///get-started/setup/</uri>
      <uri>http://www.w3.org/1999/02/22-rdf-syntax-ns#_1</uri>
