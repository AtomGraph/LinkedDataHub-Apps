    <triple>
      <uri>file:///user-guide/add-data/</uri>
      <uri>http://purl.org/dc/terms/description</uri>
      <plainLiteral>Fork RDF data from Linked Data resources and SPARQL results</plainLiteral>
    </triple>
    <triple>
      <uri>file:///user-guide/add-data/</uri>
      <uri>http://www.w3.org/1999/02/22-rdf-syntax-ns#_1</uri>
