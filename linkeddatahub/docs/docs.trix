    </triple>
    <triple>
      <uri>file:///reference/user-interface/</uri>
      <uri>http://rdfs.org/sioc/ns#has_container</uri>
      <uri>file:///reference/</uri>
    </triple>
    <triple>
      <uri>file:///reference/user-interface/</uri>
