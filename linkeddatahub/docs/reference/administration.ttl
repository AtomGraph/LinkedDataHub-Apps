    dct:description "Access control and domain model management" ;
    apl:content <#content> .

<#content> a apl:Content ;
    rdf:rest () ;
    rdf:first """<div xmlns="http://www.w3.org/1999/xhtml">
    <p class="lead">Access control and domain model management</p>
    <p>Administration is provided in a separate LDT application, which is paired with each <a href="../dataspace/#end-user-apps">end-user
        application</a>.</p>
</div>"""^^rdf:XMLLiteral .