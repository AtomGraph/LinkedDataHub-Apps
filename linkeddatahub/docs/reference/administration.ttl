@prefix def:    <https://w3id.org/atomgraph/linkeddatahub/default#> .
@prefix ldh:    <https://w3id.org/atomgraph/linkeddatahub#> .
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .
@prefix dh:     <https://www.w3.org/ns/ldt/document-hierarchy#> .
@prefix dct:    <http://purl.org/dc/terms/> .
@prefix sioc:   <http://rdfs.org/sioc/ns#> .
@prefix spin:	<http://spinrdf.org/spin#> .

<> a dh:Container ;
    sioc:has_parent <..> ;
    dct:title "Administration" ;
    dct:description "Access control and domain model management" ;
    rdf:_1 <#content> ;
    rdf:_2 <#select-children> .

<#content> a ldh:XHTML ;
    rdf:value """<div xmlns="http://www.w3.org/1999/xhtml">
    <p class="lead">Access control and domain model management</p>
    <p>Administration is provided in a separate LinkedDataHub application, which is paired with each <a href="../dataspace/#end-user-apps">end-user
        application</a>.</p>
</div>"""^^rdf:XMLLiteral .

<#select-children> a ldh:Object ;
    rdf:value ldh:ChildrenView .
