@prefix def:    <https://w3id.org/atomgraph/linkeddatahub/default#> .
@prefix ldh:    <https://w3id.org/atomgraph/linkeddatahub#> .
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .
@prefix dh:     <https://www.w3.org/ns/ldt/document-hierarchy#> .
@prefix dct:    <http://purl.org/dc/terms/> .
@prefix sioc:   <http://rdfs.org/sioc/ns#> .

<user-guide/create-data/create-instances/> a dh:Item ;
    sioc:has_container <user-guide/create-data/> ;
    dct:title "Create instances" ;
    dct:description "Learn how to create of built-in and user-defined classes" ;
    rdf:_1 <#content> .
    
<#content> a ldh:Content ;
    rdf:value """<div xmlns="http://www.w3.org/1999/xhtml">
    <p class="lead">Learn how to create of built-in and user-defined classes</p>
    <div>
        <h2>Built-in classes</h2>
        <p></p>
    </div>
    <div>
        <h2>User-defined classes</h2>
        <p>Follow these steps:</p>
        <ol>
            <li>Click the <span class="btn btn-primary">Create</span> dropdown in the top-left corner</li>
            <li>Click on <samp>Instance</samp> in the drop-down list that appears. An editing form will open.</li>
            <li>In the resource of <samp>Type</samp> <samp>Instance</samp>, click on the <samp>Instance</samp> typeahead input, type <samp>Concept</samp> and select the class from the list</li>
            <li>Fill out the mandatory fields in the form that appeared:
                <dl>
                    <dt>Preferred label</dt>
                    <dd>Enter concept label</dd>
                    <dt>Title</dt>
                    <dd>Enter document title</dd>
                </dl>
            </li>
            <li>Click <span class="btn btn-primary">Save</span></li>
        </ol>
        <p>A concept and its item should appear with the URI <samp>https://localhost:4443/concepts/{slug}/</samp>, depending on the optional URI slug value (a unique UUID string is assigned by default).</p>
    </div>
</div>"""^^rdf:XMLLiteral .




        
        
